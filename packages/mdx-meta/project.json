{
  "name": "mdx-meta",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/mdx-meta/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/mdx-meta",
        "main": "packages/mdx-meta/src/index.ts",
        "tsConfig": "packages/mdx-meta/tsconfig.lib.json",
        "assets": ["packages/mdx-meta/*.md"]
      }
    },
    "publish": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node tools/scripts/publish.mjs mdx-meta {args.ver} {args.tag}"
      },
      "dependsOn": [
        {
          "projects": "self",
          "target": "build"
        }
      ]
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/mdx-meta/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/mdx-meta"],
      "options": {
        "jestConfig": "packages/mdx-meta/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
